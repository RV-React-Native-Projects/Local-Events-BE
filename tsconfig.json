{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./src/",
    "paths": {
      "@db/*": ["db/*"],
      "@utils/*": ["utils/*"],
      "@controllers/*": ["controllers/*"],
      "@middlewares/*": ["middlewares/*"],
      "@routes/*": ["routes/*"],
      "@types/*": ["types/*"],
      "@services/*": ["services/*"],
      "@config/*": ["config/*"],
      "@lib/*": ["lib/*"],
      "@hooks/*": ["hooks/*"]
    }
  }
}